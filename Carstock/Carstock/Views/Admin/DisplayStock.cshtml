@model IEnumerable<Car>
@{
	ViewData["Title"] = "Stock";
	//var carmodels = ViewData["Carmodel"] as List<Carmodel>;
}

<h2>Stock</h2>

<table class="table">
	<thead class="thead-dark">
		<tr>
			<th>Image</th>
			<th>Brand</th>
			<th>Model</th>
			<th>Price</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var car in Model)
		{
			<tr>
			<td><img src="@car.IdModelNavigation.Image" style="height:50px; width:75px;"/></td>
			<td>@car.IdModelNavigation.Brand</td>
			<td>@car.IdModelNavigation.Model</td>
			<td>@car.IdModelNavigation.Price €</td>
			<td><a class="btn btn-danger" asp-controller="Admin" asp-action="RemoveCar" asp-route-id="@car.IdCar">Delete</a></td>
			</tr>
		}
	</tbody>
	</table>
































